{
  "name": "vonion",
  "version": "0.0.1",
  "description": "Vue components, layer by layer",
  "keywords": [
    "vue",
    "components",
    "typescript"
  ],
  "homepage": "https://github.com/anthonkendel/vonion#readme",
  "bugs": {
    "url": "https://github.com/anthonkendel/vonion/issues"
  },
  "author": {
    "name": "Anthon",
    "email": "anthon.kendel@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonkendel/vonion.git"
  },
  "scripts": {
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run lint && npm run test:unit",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@arbetsmyra/commitlint-config": "^1.3.0",
    "@arbetsmyra/prettier-config": "^1.2.0",
    "@arbetsmyra/semantic-release-config": "^1.0.3",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "bulma": "^0.8.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/?(*.)+(test).[jt]s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ]
  },
  "renovate": {
    "extends": [
      "@arbetsmyra"
    ]
  },
  "prettier": "@arbetsmyra/prettier-config",
  "commitlint": {
    "extends": [
      "@arbetsmyra"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix"
    }
  },
  "release": {
    "extends": [
      "@arbetsmyra/semantic-release-config"
    ]
  }
}
