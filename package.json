{
  "name": "vonion",
  "version": "1.0.0",
  "description": "Vue components, layer by layer",
  "keywords": [
    "vue",
    "components",
    "typescript"
  ],
  "homepage": "https://github.com/anthonkendel/vonion#readme",
  "bugs": {
    "url": "https://github.com/anthonkendel/vonion/issues"
  },
  "author": {
    "name": "Anthon",
    "email": "anthon.kendel@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonkendel/vonion.git"
  },
  "scripts": {
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run lint && npm run test:unit",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "3.11.2",
    "luxon": "1.26.0",
    "vue": "2.6.12",
    "vue-router": "3.5.1"
  },
  "devDependencies": {
    "@arbetsmyra/commitlint-config": "2.1.0",
    "@arbetsmyra/prettier-config": "1.2.1",
    "@arbetsmyra/semantic-release-config": "1.1.19",
    "@commitlint/cli": "12.1.1",
    "@types/jest": "26.0.23",
    "@types/luxon": "1.26.5",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.12",
    "@vue/cli-plugin-typescript": "4.5.12",
    "@vue/cli-plugin-unit-jest": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "husky": "4.3.8",
    "prettier": "2.2.1",
    "sass": "1.32.12",
    "sass-loader": "10.1.1",
    "typescript": "4.2.4",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/?(*.)+(test).[jt]s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ]
  },
  "renovate": {
    "extends": [
      "@arbetsmyra"
    ]
  },
  "prettier": "@arbetsmyra/prettier-config",
  "commitlint": {
    "extends": [
      "@arbetsmyra"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix && npm run build && git add docs/"
    }
  },
  "release": {
    "extends": [
      "@arbetsmyra/semantic-release-config"
    ]
  }
}
